$(document).ready(function() {

  $('.star-rating').raty({
    half     : true,
    starHalf : '<%= asset_path 'star-half.png' %>',
    starOff  : '<%= asset_path 'star-off.png' %>',
    starOn   : '<%= asset_path 'star-on.png' %>',
    readOnly : true,
    score: function() {
      return $(this).attr('data-score');
    }
  });

  
  $('#star-rating-professionalism').raty({
   path: '/assets/',
   scoreName: 'rating[professionalism_rating]'
 });

  $('#star-rating-quality').raty({
   path: '/assets/',
   scoreName: 'rating[quality_rating]'
 });

});